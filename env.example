# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Server Configuration
PORT=3000

# Supabase Configuration
# Dapatkan dari: Supabase Dashboard > Project Settings > API
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_role_key_here

# IoT Device Configuration (ESP32)
ESP32_HOST=http://192.168.1.20
# Cara menemukan IP: Upload kode ESP32, buka Serial Monitor, lihat IP Address yang tercetak

# YouTube API (opsional, untuk hasil real-time)
YOUTUBE_API_KEY=your_youtube_data_api_key_here
